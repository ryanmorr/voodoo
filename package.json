{
  "name": "@ryanmorr/voodoo",
  "description": "An experimental reactive execution context inspired by Svelte",
  "version": "0.1.0",
  "license": "Unlicense",
  "author": "Ryan Morr",
  "homepage": "https://github.com/ryanmorr/voodoo",
  "repository": "github:ryanmorr/voodoo",
  "bugs": "https://github.com/ryanmorr/voodoo/issues",
  "keywords": [
    "javascript",
    "reactive",
    "execution context",
    "proof of concept"
  ],
  "type": "module",
  "main": "dist/cjs/voodoo.js",
  "module": "dist/esm/voodoo.js",
